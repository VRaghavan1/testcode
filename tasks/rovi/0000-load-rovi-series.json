{
  "tasks": [
    {
      "taskCode": "CreateDataCollection-rovi-series-source",
      "taskClass": "CreateDataCollection",
      "taskConfigurationTemplate": {
        "in": "{FOUNDATION_DATA}/rovi",
        "params": {
          "customerCode": "rogers",
          "dataRepositoryCode": "eclipse-dev",
          "dataCollectionCode": "rovi-series-source",
          "dataRecordIdentifierProperty": "id",
          "name": "Dwayne's Collection",
          "successIfExists": true
        }
      }
    }
    ,
    {
      "taskCode": "TruncateDataCollection-rovi-series-source",
      "taskClass": "TruncateDataCollection",
      "taskConfigurationTemplate": {
        "params": {
          "customerCode": "rogers",
          "dataRepositoryCode": "eclipse-dev",
          "dataCollectionCode": "rovi-series-source"
        }
      }
    }
    ,
    {
      "taskCode": "CsvFileFolderToDataRecord",
      "taskConfigurationTemplate": {
        "params": {
          "file_pattern": "rovi_series.csv",
          "value_delimeter": "|",
          "field_names": [
            "series_id",
            "full_title",
            "synopsis",
            "delta"
          ]
        }
      }
    }
